    "bp_gender.index = ['Female' if i==0 else 'Male' for i in bp_gender.index]\n",
    "bp_gender.plot(kind='bar',color=['pink','lightblue'])\n",
    "plt.title(\"High Blood Pressure Prevalence by Gender\")\n",
    "plt.ylabel('Percentage (%)')\n",
    "plt.xticks(rotation=0)\n",
    "plt.show()"
