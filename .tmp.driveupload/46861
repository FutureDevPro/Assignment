    "smoking_death = df.groupby('smoking')['DEATH_EVENT'].mean()*100\n",
    "smoking_death.index = ['smoking' if i==1 else 'non_smoking' for i in smoking_death.index]\n",
    "smoking_death.plot(kind='bar',color=['green','red'])\n",
    "plt.title('Heart Failure (Death Event) by Smoking Habit')\n",
    "plt.ylabel('Death Event Rate (%)')\n",
    "plt.xticks(rotation=0)\n",
    "plt.show()"
